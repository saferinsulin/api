[
  {
    "type": "get",
    "url": "/v1/check/:governance",
    "title": "Governance hexcode check",
    "name": "GetCheck",
    "group": "GET",
    "version": "1.2.3",
    "parameter": {
      "fields": {
        "Parameter": [
          {
            "group": "Parameter",
            "type": "String",
            "optional": false,
            "field": "governance",
            "description": "<p>Governance hexcode generated by the saferinsulin.org algorithm</p>"
          }
        ]
      }
    },
    "success": {
      "fields": {
        "Success 200": [
          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "function",
            "description": "<p>Calculator function called: 'a' (start) or 'b' (continue).</p>"
          },
          {
            "group": "Success 200",
            "type": "Number",
            "optional": false,
            "field": "current",
            "description": "<p>Current glucose parameter passed to function</p>"
          },
          {
            "group": "Success 200",
            "type": "Number",
            "optional": false,
            "field": "last",
            "description": "<p>Previous glucose parameter passed to function [b only]</p>"
          },
          {
            "group": "Success 200",
            "type": "Number",
            "optional": false,
            "field": "rate",
            "description": "<p>Rate parameter passed to function [b only]</p>"
          },
          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "date",
            "description": "<p>Date the function was invoked</p>"
          }
        ]
      }
    },
    "error": {
      "fields": {
        "Error 4xx": [
          {
            "group": "Error 4xx",
            "optional": false,
            "field": "InvalidGovernanceCode",
            "description": "<p>API was supplied a missing or invalid governance code which could not be processed</p>"
          }
        ]
      }
    },
    "filename": "routes/check.js",
    "groupTitle": "GET"
  },
